<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Text Jet</name>
    <description>
        A sample chat application
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Paweł Jastrzębski, Igor Sowiński
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon dentisy="xxxhdpi" src="/res/icon/android/icon-192-xxxhdpi.png" />
        <icon dentisy="xxhdpi" src="/res/icon/android/icon-144-xxhdpi.png" />
        <icon dentisy="xhdpi" src="/res/icon/android/icon-96-xhdpi.png" />
        <icon dentisy="hdpi" src="/res/icon/android/icon-72-hdpi.png" />
        <icon dentisy="mdpi" src="/res/icon/android/icon-48-mdpi.png" />
        <icon dentisy="idpi" src="/res/icon/android/icon-36-ldpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Fullscreen" value="true" />
    <engine name="android" spec="^7.1.4" />
    <engine name="browser" spec="^5.0.4" />
</widget>
